{
    "name": "the-wall",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "watch-sass": "sass --no-source-map --watch public/stylesheets/style.scss public/stylesheets/style.css",
        "watch-nodemon": "nodemon -L --exec babel-node --inspect=0.0.0.0:9229 server/app.js",
        "dev": "npm-run-all --parallel watch-sass watch-nodemon"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "ejs": "~2.6.1",
        "express": "~4.16.1",
        "express-session": "^1.17.3",
        "express-validator": "^6.14.2",
        "http-errors": "~1.6.3",
        "jquery": "^3.6.1",
        "memorystore": "^1.6.7",
        "morgan": "~1.9.1",
        "mysql": "^2.18.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.19.6",
        "@babel/node": "^7.20.0",
        "@babel/preset-env": "^7.19.4",
        "nodemon": "^2.0.20",
        "npm-run-all": "^4.1.5",
        "sass": "^1.55.0"
    }
}
